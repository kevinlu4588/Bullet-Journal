<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<Scene fx:id="fileChooserScene" fill="transparent" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <root>
    <DialogPane fx:id="fileChooserDialogPane" contentText="dlkfsln" headerText="kjlkdjfljsd" prefHeight="194.0" prefWidth="736.0">
      <buttonTypes>
        <ButtonType fx:constant="FINISH" />
        <ButtonType fx:constant="CANCEL" />
      </buttonTypes>
      <content>
        <HBox fx:id="fileChooserHbox" prefHeight="1.0" prefWidth="223.0">
          <children>
            <Button fx:id="fileChooserButton" mnemonicParsing="false" prefHeight="34.0" prefWidth="144.0" text="Choose file...">
              <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
              </HBox.margin>
            </Button>
            <TextField fx:id="fileChooserTextField" prefHeight="34.0" prefWidth="557.0" HBox.hgrow="ALWAYS">
              <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
              </HBox.margin>
            </TextField>
          </children>
        </HBox>
      </content>
      <header>
        <Label fx:id="fileChooserHeader" style="-fx-font-weight: bold; -fx-font-size: 16;" text="File Chooser">
          <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </padding>
        </Label>
      </header>
      <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
      </padding>
    </DialogPane>
  </root>
</Scene>
